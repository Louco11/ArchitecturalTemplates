  "path": "/templates/TestTemplate",
  "param": [
    "name"
  ],
  "selectParam": [
    {
      "paramName": "ide",
      "paramValue": [
        "AndroidStudio",
        "Pycharm",
        "intellij"
      ]
    },
    {
      "paramName": "version",
